<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.ticket.product.ProductVO" alias="product"/>
		<typeAlias type="com.ticket.book.BookVO" alias="book"/>
		<typeAlias type="com.ticket.member.MemberVO" alias="member"/>
		<typeAlias type="com.ticket.round.RoundVO" alias="round"/>
		<typeAlias type="com.ticket.discnt.DiscntVO" alias="discnt"/>
		<typeAlias type="com.ticket.expect.ExpectVO" alias="expect"/>
		<typeAlias type="com.ticket.review.ReviewVO" alias="review"/>
		<typeAlias type="com.ticket.seatrate.SeatRateVO" alias="seatRate"/>
		<typeAlias type="com.ticket.admin.AdminVO" alias="admin"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="config/mybatis/mapper/product.xml"/>
		<mapper resource="config/mybatis/mapper/book.xml"/>
		<mapper resource="config/mybatis/mapper/member.xml"/>
		<mapper resource="config/mybatis/mapper/round.xml"/>
		<mapper resource="config/mybatis/mapper/discnt.xml"/>
		<mapper resource="config/mybatis/mapper/expect.xml"/>
		<mapper resource="config/mybatis/mapper/review.xml"/>
		<mapper resource="config/mybatis/mapper/seatRate.xml"/>
		<mapper resource="config/mybatis/mapper/admin.xml"/>
	</mappers>
</configuration>